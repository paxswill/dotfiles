            ]
        },
        {
            "command": "nextTab",
            "keys": [
                "ctrl+pgdn"
            ]
        },
        {
            "command": "openSettings",
            "keys": [
                "ctrl+,"
            ]
        },
        {
            "command": "paste",
            "keys": [
                "ctrl+shift+v"
            ]
        },
        {
            "command": "prevTab",
