###########################################
#      DO NOT EDIT THIS FILE DIRECTLY     #
# EDIT ~/.dotfiles/src/.gitconfig INSTEAD #
# #########################################
[rerere]
	enabled = 1
[merge]
	tool = opendiff
	defaultToUpstream = true
[user]
	name = Will Ross
ifdef(`NRL',
`	email = wross@cmf.nrl.navy.mil',dnl
`	email = paxswill@gmail.com'dnl
)
[core]
	excludesfile = ~/.cvsignore
[color]
	ui = auto
[diff]
	renames = copies
[github]
	user = paxswill
[alias]
	br = branch
	ci = commit
	co = checkout
	dc = diff --cached
	df = diff
	dh = diff HEAD
	ff = merge --ff-only
	lg = log --graph --decorate --oneline
	st = status
