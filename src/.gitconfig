###########################################
#      DO NOT EDIT THIS FILE DIRECTLY     #
# EDIT ~/.dotfiles/src/.gitconfig INSTEAD #
# #########################################
[rerere]
	enabled = 1
[merge]
	tool = opendiff
	defaultToUpstream = true
	log = 20
[user]
	name = Will Ross
	email = EMAIL
[core]
	excludesfile = ~/.cvsignore
[color]
	ui = auto
[diff]
	renames = copies
[github]
	user = paxswill
[alias]
	br = branch
	ci = commit
	co = checkout
	dc = diff --cached
	df = diff
	dh = diff HEAD
	ff = merge --ff-only
	lg = log --graph --decorate --oneline
	st = status
	ap = add --patch
[credential]
ifdef(`OSX',`	helper = osxkeychain',`dnl')
	helper = cache --socket /tmp/USER/git-credential-cache-socket
[push]
	default = current
