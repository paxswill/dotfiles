### Private IP Addresses ###
Host 10.*,172.16.*,172.17.*,172.18.*,172.19.*,172.2?.*,172.30.*,172.31.*,192.168.*
	CheckHostIP no
	Ciphers arcfour,arcfour128,arcfour256,aes128-ctr,aes192-ctr,
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null

### Global Config ***
Host *
ifdef(`SSH_HAS_CONTROL_MASTER',
`	ControlMaster auto
	ControlPath /tmp/ssh_mux_%l_%h_%p_%r',
)
ifdef(`SSH_HAS_CONTROL_PERSIST',
`	ControlPersist 15m'
)
	CheckHostIP yes
ifdef(`SSH_HAS_EXIT_ON_FORWARD_FAILURE',
`	ExitOnForwardFailure yes'
)
	ForwardAgent yes
ifdef(`SSH_HAS_HASH_KNOWN_HOSTS',
`	HashKnownHosts yes',
`	UserKnownHostsFile ~/.ssh/known_hosts.unhashed'
)
